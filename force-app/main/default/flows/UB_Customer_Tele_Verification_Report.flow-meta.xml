    <formulas>
        <description>Concatenate Vehicle Make Model</description>
        <name>ConcatenateVehicleMakeModel</name>
        <dataType>String</dataType>
        <expression>{!Iterate_Vehicle_Records.Make__c}+&quot;-&quot;+{!Iterate_Vehicle_Records.Model__c}</expression>
    </formulas>
    <formulas>
        <description>Current Logged In User</description>
        <name>CurrentUser</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!$User.FirstName})), {!$User.FirstName}+&apos; &apos;, &apos;&apos;)  + IF(NOT(ISBLANK({!$User.MiddleName})), {!$User.MiddleName}+&apos; &apos;, &apos;&apos;)   + {!$User.LastName}</expression>
    </formulas>
    <formulas>
        <description>DisplayVehicleDetails</description>
        <name>DisplayVehicleDetails</name>
        <dataType>String</dataType>
        <expression>MID(LEFT({!VehicleMakeModelCollection}, LEN({!VehicleMakeModelCollection})-1), 2, LEN(LEFT({!VehicleMakeModelCollection}, LEN({!VehicleMakeModelCollection})-1)))</expression>
    </formulas>
    <formulas>
        <description>Applicant Full Address</description>
        <name>FullAddress</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISNULL({!Fetch_Applicant_Address.Id})), {!Fetch_Applicant_Address.Name}+&quot; &quot;+{!Fetch_Applicant_Address.Address_Line_2__c}+&quot; &quot;+{!Fetch_Applicant_Address.Address_Line_3__c}+&quot; &quot;+{!Fetch_Applicant_Address.Pincode__r.City__r.Name}+&quot; &quot;+{!Fetch_Applicant_Address.District__c}+&quot; &quot;+{!Fetch_Applicant_Address.State__c}+&quot; &quot;+{!Fetch_Applicant_Address.Country__c}+&quot; &quot;+{!Fetch_Applicant_Address.Pincode__r.Name}, &quot; &quot;)</expression>
    </formulas>
    <formulas>
        <name>IsTypeApplicant</name>
        <dataType>Boolean</dataType>
        <expression>IF(ISPICKVAL({!Fetch_Loan_Applicant_Details.Applicant_Type__c},&quot;Applicant&quot;), true, false)</expression>
    </formulas>
    <formulas>
        <description>Dynamic Report Sub-Header</description>
        <name>ReportSubHeader</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!Fetch_Loan_Applicant_Details.Applicant_Type__c},&quot;Applicant&quot;), &quot;Applicant To Be Called From Office Land Line Only&quot;, &quot;Co-Applicant To Be Called From Office Land Line Only &quot;)</expression>
