                    </tr>
                </table> 
                <apex:outputPanel rendered="{!If(wrapper.isApplicantOfficeAdd,true,false)}">
                    <div class="textSize" height="20px" style="background-color:#D8B4FE;color:black;text-align:left;" >
                        Company Office Address 
                    </div>
                </apex:outputPanel>
                <table style="width:100%;font-size:12px; " > 
                    <apex:repeat value="{!applicantOfficeAddressList }" var="address">
