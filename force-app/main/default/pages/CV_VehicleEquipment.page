    <body >
        <apex:form >
            
            <table border="1">
                <tr>
                    <td class="col-tab-1" >
                        <apex:image id="theImage1" value="{!$Resource.UBLogo}" width="120" height="110" alt="Description of image here"/>
                    </td>
                    <td class="col-tab-5" >
                        <div >
                            <table class="align-center" >
                                <tr>
                                    <td class="text-align"  ><h3> Vehicle &amp; Equipment Loans </h3></td>
                                </tr>
                                <tr>
                                    <td class="text-align" > 
                                        <input type="date"/> 
                                    </td>
                                    
                                </tr>
                                <tr>
                                    <td class="text-align" style="text-align: right;" >
                                        Please Select:  <input type="checkbox"/> MUDRA <input type="checkbox"/> STAND-UP INDIA 
                                    </td> 
                                </tr>
                            </table>
                            <br/> 
                        </div>
                    </td>
                    <td class="col-tab-4" > 
                        <div >
                            <table class="textSize" >
                                <tr>
                                    <td class="text-align"  ></td>
                                </tr>
                                <tr>
                                    <td class="text-align" > 
                                        Sourcing  <input type="checkbox"/> Business Channel <input type="checkbox"/> Direct Sales Team <input type="checkbox"/> Branch ({!loanApplication.Lead__r.Branch__r.Name}) 
                                    </td>
                                    
                                </tr>
                                <tr>
                                    <td class="text-align"  >
                                        Channel Code / Employee Code  <input type="text" /> / <input type="text"/> 
                                    </td> 
                                </tr>
                                <tr>
                                    <td class="text-align"  >
                                        Application Number  {!loanApplication.Name}
                                    </td> 
                                </tr>
                            </table>
                        </div>
                        
                    </td>
                </tr>
            </table>
            <div class="slds-section">
