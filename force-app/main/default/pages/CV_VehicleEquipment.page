                            <td > <div class="label-style"><b> City/Town/Village*: </b> </div>  <div class="value-style">{!address.City__c}</div></td>
                        </tr>
                        <tr>
                            <td > <div class="label-style"><b> Landmark: </b> </div>  <div class="value-style">{!address.Landmark__c}</div></td>
                            <td > <div class="label-style"><b>District*: </b> </div>  <div class="value-style"> {!address.District__c}</div></td>
                            <td > <div class="label-style"><b>Pin/Post Code: </b> </div>  <div class="value-style">{!address.Pincode__r.Name}</div></td>
                        </tr>
                        <tr>
                            <td > <div class="label-style"><b>State*:  </b> </div>  <div class="value-style"> {!address.State__c}</div></td>
                            <td > <div class="label-style"><b> </b> </div>  <div class="value-style"></div></td>
                            <td > <div class="label-style"><b> </b> </div>  <div class="value-style"></div></td>
                        </tr>
                    </apex:repeat>
                </table> 
                <table> 
                    <apex:repeat value="{!applicantCurrentAddressList }" var="address">
