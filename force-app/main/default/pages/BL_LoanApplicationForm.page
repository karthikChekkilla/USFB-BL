                    <table border="0" cellspacing="0">
                        <tr>
                            <!--Added by Mohit on August 27th, 2024 (Starts)-->
                            <td> <div class="slds-div-style"> <div class="label-style"><b> Flat No. &#38; Bidg. Name  </b> </div><br/><div class="value-style">{!primeAppOfficeAddress.Name}</div> </div></td>
