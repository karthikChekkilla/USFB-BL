    <body >
        
        <apex:form >
            <table border="0" cellspacing="0">
                <tr>
                    <td style="border:1px solid black;width:25%">
                        <div>
                            <apex:image id="theImage1" value="{!$Resource.UBLogo}" width="100%" style="height:70px;" alt="Description of image here"/>
                        </div>
                    </td>
                    <td style="border:1px solid black;width:50%">
                        <div style="text-align:center;width:100%;margin-top:-10px;">
                            <h5 style="margin-bottom:5px">LOAN APPLICATION FORM</h5>
                            <div style="font-size:13px"> 
                                (For Resident  Applicant) 
                            </div>
                        </div>
                    </td>
                    <td style="border:1px solid black;width:25%">
                        <div>
                            <div class="text-align" style="font-weight:bold">Application Number </div>
                            <div class="text-align" style="font-size:12px">{!loanApplication.Name}</div>
                        </div>
                    </td>
                </tr>               
            </table>
            
            <br/>
            <!-- Section 1 -->
            <div class="slds-section">               
                <table border="0" cellspacing="0">
                    <tr>
                        <td> <div class="slds-div-style"> <div class="label-style"><b>Branch Name  </b> </div><br/><div class="value-style">{!loanApplication.Branch__r.Name}</div> </div></td>
                        <td> <div class="slds-div-style"> <div class="label-style"><b> Branch Code </b> </div><br/><div class="value-style"> {!loanApplication.Branch__r.Branch_Code__c}</div> </div></td>
                        <!--Added by Mohit on August 27th, 2024-->
                        <td><div class="slds-div-style"> <div class="label-style"><b> Product Code </b> </div><br/><div class="value-style">{!loanApplication.Products__r[0].Product__r.Product_Code__c}</div> </div></td>
                    </tr>
                </table>
                <table>
                    <tr>
                        <td>  <div class="label-style"><b>I / We hereby request you to sanction the loan based on the following :  </b> </div></td>
                    </tr>
                </table> 
                <table>
                    <tr>
                        <td> <div class="slds-div-style"> <div class="label-style"><b> Application Date  </b> </div><br/><div class="value-style"><apex:outputText value="{0,date,dd/MM/yyyy}"><apex:param value="{!loanApplication.CreatedDate}"/></apex:outputText></div> </div></td>
                        <!--Added by Mohit on August 27th, 2024-->
                        <td> <div class="slds-div-style"> <div class="label-style"><b> Type of Loan </b> </div><br/><div class="value-style"> </div>Business Loan</div></td>
                        <!--Added by Mohit on August 27th, 2024-->
                        <td><div class="slds-div-style"> <div class="label-style"><b>Purpose of Loan </b> </div><br/><div class="value-style">{!loanApplication.Products__r[0].BL_Purpose_of_Loan__c}</div> </div></td>
                    </tr>
                    <tr>
                        <td> <div class="slds-div-style"> <div class="label-style"><b> Loan Amount Requested  </b> </div><br/><div class="value-style">{!loanApplication.Products__r[0].BL_Applied_Loan_Amount__c}</div> </div></td>
                        <td> <div class="slds-div-style"> <div class="label-style"><b>Interest Type </b> </div><br/><div class="value-style"> </div> </div>{!loanApplication.Products__r[0].BL_Interest_Type__c}</td>
                        <td><div class="slds-div-style"> <div class="label-style"><b>  Loan Tenure(In months) </b> </div><br/><div class="value-style">{!loanApplication.Products__r[0].BL_Loan_Tenure_months__c}</div> </div></td>
                    </tr>
                    <tr>
                        <td> <div class="slds-div-style"> <div class="label-style"><b> Repayment Mode  </b> </div><br/><div class="value-style">{!loanApplication.BL_Repayment_Mode__c}</div> </div></td>
                        <td> <div class="slds-div-style"> <div class="label-style"><b> </b> </div><br/><div class="value-style"> </div> </div></td>
                        <td><div class="slds-div-style"> <div class="label-style"><b> </b> </div><br/><div class="value-style"></div> </div></td>
                    </tr>
                    
                </table>
            </div>
            <!-------------------------- Section 1 End------------------------------->
            
            <br/>
            <!---------------------------------------- Section  2 - A. Non Individual details - applicant ------------------------------>
            <apex:outputPanel rendered="{!If(wrapper.isApplicantNonIndividual,true,false)}">
                <div class="slds-section">
                    <h3 class="slds-section__title slds-theme_shade">
                        <span class="header slds-p-horizontal_small slds-text-color_inverse slds-text-align_center">A. Non Individual details - applicant</span>
                    </h3>
                    
                    <table border="0" cellspacing="0">
                        <tr>
                            <!--Added by Mohit on August 27th, 2024-->
