<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Upload_Report_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(Vendor_Status__c),ISPICKVAL(Vendor_Status__c, &apos;Completed&apos;),Is_Report_Uploaded__c = false)</errorConditionFormula>
    <errorMessage>Please upload report first to mark as complete status!!</errorMessage>
</ValidationRule>
