<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Number_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISNUMBER( Account_Number__c ) )
)</errorConditionFormula>
    <errorDisplayField>Account_Number__c</errorDisplayField>
    <errorMessage>Account Number accept only numeric value</errorMessage>
</ValidationRule>
